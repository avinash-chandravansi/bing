{
    "version": 2,
    "functions": {
      "server.js": {
        "memory": 1024,
        "includeFiles": "data.json"
      }
    },
    "builds": [
      {
        "src": "server.js",
        "use": "@now/node"
      },
      { "src": "*.json", "use": "@vercel/public" }
    ],
    "routes": [
      {
      
        "src": "/(.*)",
        "dest": "server.js"
      }
    ]
  }