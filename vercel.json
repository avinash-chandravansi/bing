{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@now/node"
      },
      { "src": "*.json", "use": "@vercel/public" }
    ],
    "routes": [
      {
      
        "src": "/(.*)",
        "dest": "server.js"
      }, { "src": "/getdata", "methods": ["GET"], "dest": "/data.json" }
    ]
  }